// css reset
html,body,div,span,object,iframe,h1,h2,h3,p,blockquote,pre,a,code,em,img,small,strong,ol,ul,li,fieldset,form,label,legend,table,caption,article,aside,canvas,figure,figcaption,footer,header,menu,nav,section,summary,time,audio,video
  border:0;font-size:100%;font:inherit;margin:0;padding:0

article,aside,details,figcaption,figure,footer,header,menu,nav,section
  display block

body
  line-height 1

ol,ul
  list-style none

blockquote,q
  quotes none

blockquote:before,blockquote:after,q:before,q:after
  content none

table
  border-collapse:collapse;border-spacing:0